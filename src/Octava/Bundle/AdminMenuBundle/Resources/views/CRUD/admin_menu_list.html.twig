{% extends 'OctavaAdminBundle:CRUD:list.html.twig' %}

{% block table_body %}
    <tbody data-no-turbolink>
    {% for object in admin.getMenuTree() %}
        <tr id="row_{{ object.id }}">
            {% include admin.getTemplate('inner_list_row') %}
        </tr>
    {% endfor %}
    </tbody>
{% endblock %}

{% block side_bar_before_nav %}
    <div>
        {{ admin.getTreeManager().get|raw }}
    </div>
{% endblock %}

{% block table_footer '' %}
{% block pager_links '' %}